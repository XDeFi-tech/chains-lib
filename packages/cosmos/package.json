{
  "name": "@xdefi-tech/chains-cosmos",
  "version": "2.0.62",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "type": "commonjs",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "devDependencies": {
    "@esbuild-plugins/node-modules-polyfill": "0.2.2",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/client-preset": "2.1.1",
    "@graphql-codegen/named-operations-object": "2.3.1",
    "@graphql-codegen/near-operation-file-preset": "2.5.0",
    "@graphql-codegen/typescript-document-nodes": "3.0.2",
    "eslint-config-custom": "*",
    "graphql": "16.6.0",
    "jest": "27.5.1",
    "jest-environment-jsdom": "27.5.1",
    "jest-watch-typeahead": "1.0.0",
    "ts-jest": "27.1.4",
    "ts-proto": "1.181.1",
    "tsup": "6.6.3",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "@cosmjs/amino": "0.32.3",
    "@cosmjs/cosmwasm-stargate": "0.32.3",
    "@cosmjs/encoding": "0.32.3",
    "@cosmjs/crypto": "0.32.3",
    "@cosmjs/launchpad": "0.27.1",
    "@cosmjs/ledger-amino": "0.32.3",
    "@cosmjs/proto-signing": "0.32.3",
    "@cosmjs/stargate": "0.32.3",
    "@cosmjs/tendermint-rpc": "0.32.3",
    "@cosmology/lcd": "0.13.4",
    "@cosmos-client/core": "0.47.1",
    "@keplr-wallet/cosmos": "0.12.82",
    "@keplr-wallet/crypto": "0.12.82",
    "@ledgerhq/hw-transport": "6.30.3",
    "@ledgerhq/hw-transport-webhid": "6.28.3",
    "@tanstack/react-query": "5.51.1",
    "@xdefi-tech/chains-core": "*",
    "axios": "1.4.0",
    "bech32": "2.0.0",
    "bech32-buffer": "0.2.1",
    "bignumber.js": "9.1.2",
    "cosmjs-types": "0.9.0",
    "crypto-browserify": "3.12.0",
    "ethers": "5.6.4",
    "graphql-tag": "2.12.6",
    "lodash": "4.17.21",
    "long": "5.2.3",
    "os-browserify": "0.3.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.4.0",
    "rxjs": "7.8.0",
    "stream-browserify": "3.0.0",
    "ts-node": "10.7.0",
    "xhr2": "^0.2.1"
  },
  "scripts": {
    "build": "tsup --minify --clean",
    "watch": "tsup --watch",
    "clean": "rimraf dist .turbo node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "coverage": "jest --coverageReporters='json-summary' --coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "compile": "graphql-codegen --config codegen.yml && yarn format",
    "compile:build": "yarn compile && yarn build",
    "compile:watch": "graphql-codegen -w",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test:cicd": "jest src/coverage-test.spec.ts"
  },
  "jest": {
    "setupFiles": [
      "./jest-setup-file.ts",
      "dotenv/config"
    ],
    "preset": "ts-jest",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "testURL": "https://rpc-proxy.xdefi.services/",
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "testEnvironment": "node",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "testTimeout": 15000
  }
}
