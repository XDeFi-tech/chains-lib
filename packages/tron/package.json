{
  "name": "@xdefi-tech/chains-tron",
  "version": "2.0.39",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/XDeFi-tech/chains/",
  "license": "MIT",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "devDependencies": {
    "jest": "27.5.1",
    "jest-environment-jsdom": "27.5.1",
    "jest-watch-typeahead": "1.0.0",
    "ts-jest": "27.1.4",
    "tsup": "6.6.3",
    "typescript": "4.8.3",
    "xhr2": "0.2.1",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/client-preset": "2.1.1",
    "@graphql-codegen/named-operations-object": "2.3.1",
    "@graphql-codegen/near-operation-file-preset": "2.5.0",
    "@graphql-codegen/typescript-document-nodes": "3.0.2",
    "graphql": "16.6.0"
  },
  "dependencies": {
    "@agrozyme/types-tronweb": "5.3.2",
    "@ledgerhq/hw-app-trx": "6.28.1",
    "@ledgerhq/hw-transport": "6.30.3",
    "@ledgerhq/hw-transport-webhid": "6.28.3",
    "@xdefi-tech/chains-core": "*",
    "axios": "1.6.1",
    "bignumber.js": "9.1.2",
    "eslint-config-custom": "*",
    "ethers": "6.9.0",
    "lodash": "4.17.21",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.4.0",
    "rxjs": "7.8.0",
    "tronweb": "5.3.1",
    "ts-node": "10.7.0"
  },
  "scripts": {
    "build": "tsup --minify --clean",
    "publish:packages": "npm publish --tag $GITHUB_REF",
    "watch": "tsup --watch",
    "clean": "rimraf dist .turbo node_modules",
    "lint": "eslint .",
    "test": "jest",
    "lint:fix": "eslint . --fix",
    "test:watch": "jest --watch",
    "coverage": "jest --coverageReporters='json-summary' --coverage",
    "compile": "graphql-codegen --config codegen.yml && yarn format",
    "compile:build": "yarn compile && yarn build",
    "compile:watch": "graphql-codegen -w",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "type": "commonjs",
  "tsup": {
    "entry": [
      "src/signers/web.ts",
      "src/signers/react-native.ts",
      "src/index.ts"
    ],
    "format": "cjs",
    "splitting": false,
    "dts": true,
    "shims": false,
    "types": [
      "./dist/signers/web.d.ts",
      "./dist/signers/react-native.d.ts",
      "./dist/index.d.ts"
    ],
    "clean": true,
    "treeshake": true,
    "platform": "browser",
    "target": "ES6",
    "external": [
      "*"
    ]
  },
  "esbuildOptions": {
    "minifyWhitespace": true,
    "minifyIdentifiers": true,
    "minifySyntax": true
  },
  "jest": {
    "setupFiles": [
      "dotenv/config",
      "./jest-setup-file.ts"
    ],
    "preset": "ts-jest",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "testTimeout": 15000
  }
}
