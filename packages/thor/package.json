{
  "name": "@xdefi-tech/chains-thor",
  "version": "2.0.24",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/XDeFi-tech/chains/",
  "license": "MIT",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "devDependencies": {
    "@esbuild-plugins/node-modules-polyfill": "0.2.2",
    "eslint-config-custom": "*",
    "jest": "27.5.1",
    "jest-environment-jsdom": "27.5.1",
    "jest-watch-typeahead": "1.0.0",
    "ts-jest": "27.1.4",
    "tsup": "6.6.3",
    "typescript": "4.8.3",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/client-preset": "2.1.1",
    "@graphql-codegen/named-operations-object": "2.3.1",
    "@graphql-codegen/near-operation-file-preset": "2.5.0",
    "@graphql-codegen/typescript-document-nodes": "3.0.2",
    "graphql": "16.6.0"
  },
  "dependencies": {
    "@cosmjs/launchpad": "0.27.1",
    "@cosmos-client/core": "0.47.1",
    "@ledgerhq/hw-transport": "6.30.3",
    "@ledgerhq/hw-transport-webhid": "6.28.3",
    "@thorchain/ledger-thorchain": "0.1.0-alpha.2",
    "@xdefi-tech/chains-core": "*",
    "axios": "1.4.0",
    "bech32": "2.0.0",
    "bech32-buffer": "0.2.1",
    "bignumber.js": "9.1.2",
    "bip32": "2.0.4",
    "bip39": "3.1.0",
    "crypto-browserify": "3.12.0",
    "lodash": "4.17.21",
    "long": "5.2.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.4.0",
    "rxjs": "7.8.0",
    "stream-browserify": "3.0.0",
    "tiny-secp256k1": "2.2.3",
    "ts-node": "10.7.0"
  },
  "scripts": {
    "build": "tsup --minify --clean",
    "watch": "tsup --watch",
    "coverage": "jest --coverageReporters='json-summary' --coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf dist .turbo node_modules",
    "compile": "graphql-codegen --config codegen.yml && yarn format",
    "compile:build": "yarn compile && yarn build",
    "compile:watch": "graphql-codegen -w",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "jest": {
    "setupFiles": [
      "./jest-setup-file.ts",
      "dotenv/config"
    ],
    "preset": "ts-jest/presets/js-with-ts",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "testTimeout": 15000
  }
}
