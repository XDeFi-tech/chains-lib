{
  "name": "@xdefi-tech/chains-core",
  "version": "2.0.13",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/XDeFi-tech/chains/",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "devDependencies": {
    "@types/lodash": "4.14.182",
    "@types/uuid": "9.0.1",
    "cross-fetch": "3.1.5",
    "eslint": "8.13.0",
    "eslint-config-custom": "*",
    "jest": "27.5.1",
    "jest-environment-jsdom": "27.5.1",
    "jest-watch-typeahead": "1.0.0",
    "next-transpile-modules": "9.0.0",
    "prettier": "2.6.2",
    "ts-jest": "27.1.4",
    "tsup": "6.6.3",
    "typedoc": "0.22.15"
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@next/bundle-analyzer": "12.3.0",
    "@trezor/connect-web": "9.1.4",
    "bignumber.js": "9.1.2",
    "graphql": "16.6.0",
    "graphql-ws": "5.11.1",
    "inversify": "6.0.1",
    "lodash": "4.17.21",
    "next-compose-plugins": "2.2.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.4.0",
    "rxjs": "7.8.0",
    "sha256": "0.2.0",
    "typescript": "4.8.3",
    "uuid": "9.0.0",
    "ws": "8.8.1"
  },
  "scripts": {
    "build": "tsup --minify --clean",
    "publish:packages": "npm publish --tag $GITHUB_REF",
    "watch": "tsup --watch",
    "clean": "rimraf dist .turbo node_modules",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typedoc": "typedoc src/index.ts"
  },
  "type": "commonjs",
  "tsup": {
    "entry": [
      "src/index.ts",
      "!src/**/*.spec.*",
      "!src/custom.d.ts"
    ],
    "outDir": "dist",
    "format": "cjs",
    "splitting": false,
    "dts": true,
    "types": [
      "./dist/index.d.ts"
    ],
    "platform": "browser",
    "target": "ES6"
  },
  "jest": {
    "setupFiles": [],
    "preset": "ts-jest",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "testEnvironment": "jsdom",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "testTimeout": 15000
  }
}
