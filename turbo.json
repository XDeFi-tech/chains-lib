{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@xdefi-tech/chains-evm#build": {
      "dependsOn": ["@xdefi-tech/chains-graphql#build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "env": ["BLOCKCHAIR_API_KEY"]
    },
    "test:watch": {
      "cache": false
    },
    "coverage": {
      "dependsOn": ["build"],
      "cache": false
    },
    "compile": {
      "cache": false,
      "outputs": []
    },
    "compile:build": {
      "cache": false,
      "outputs": []
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "lint:fix": {
      "cache": false,
      "outputs": []
    },
    "clean": {
      "cache": false,
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "watch": {
      "cache": false,
      "outputs": []
    },
    "typedoc": {
      "outputs": []
    },
    "publish:packages": {
      "cache": false,
      "outputs": []
    }
  }
}
